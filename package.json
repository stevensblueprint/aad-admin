{
  "name": "aad-admin",
  "version": "0.1.0",
  "engines": {
    "node": "^20.0.0",
    "pnpm": ">=8"
  },
  "type": "module",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "db:forms": "tsx src/schemas/populateForms.ts",
    "db:seed": "prisma db seed && pnpm run types:gen && pnpm run db:forms",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "format": "prettier --write . && prisma format",
    "start": "next start",
    "types:gen": "tsx src/schemas/generateTypes.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint && pnpm run format:check && pnpm run format",
      "pre-push": "pnpm run lint && pnpm run format:check && pnpm run format"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.3.4",
    "@jsonforms/core": "^3.2.1",
    "@jsonforms/material-renderers": "^3.2.1",
    "@jsonforms/react": "^3.2.1",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/styled-engine-sc": "6.0.0-alpha.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.12.1",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.29.0",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "classnames": "^2.5.1",
    "cookies": "^0.9.1",
    "husky": "^9.0.11",
    "json-schema-to-typescript": "^13.1.2",
    "next": "^14.1.4",
    "next-auth": "^4.24.5",
    "nextjs-routes": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.2",
    "styled-components": "^6.1.8",
    "superjson": "^2.2.1",
    "typescript": "^5.4.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookies": "^0.9.0",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.1.4",
    "picsum-photos": "^3.0.10",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.3",
    "trpc-panel": "^1.3.4",
    "tsx": "^4.7.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.20.2"
  },
  "packageManager": "pnpm@8.10.2"
}
