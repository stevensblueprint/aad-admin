{
  "name": "aad-admin",
  "version": "0.1.0",
  "engines": {
    "node": "^20.0.0",
    "pnpm": ">=8"
  },
  "type": "module",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "db:forms": "tsx src/schemas/populateForms.ts",
    "db:seed": "prisma db seed && pnpm run types:gen && pnpm run db:forms",
    "db:populate": "tsx prisma/populate.ts",
    "db:stag-init": "pnpm run db:push --force-reset && pnpm run db:forms && pnpm run db:seed && pnpm run db:populate",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "format": "prettier --write . && prisma format",
    "start": "pnpm run db:stag-init && next start",
    "types:gen": "tsx src/schemas/generateTypes.ts",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint && pnpm run format:check && pnpm run format",
      "pre-push": "pnpm run lint && pnpm run format:check && pnpm run format"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@hookform/resolvers": "^3.9.1",
    "@jsonforms/core": "^3.4.1",
    "@jsonforms/material-renderers": "^3.4.1",
    "@jsonforms/react": "^3.4.1",
    "@mui/icons-material": "^6.1.8",
    "@mui/material": "^6.1.8",
    "@mui/styled-engine-sc": "6.1.8",
    "@mui/x-data-grid": "^7.22.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.61.3",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "classnames": "^2.5.1",
    "cookies": "^0.9.1",
    "husky": "^9.1.7",
    "json-schema-to-typescript": "^15.0.3",
    "next": "^15.0.3",
    "next-auth": "^4.24.10",
    "react": "18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.2",
    "styled-components": "^6.1.13",
    "superjson": "^2.2.1",
    "typescript": "^5.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@next/bundle-analyzer": "^15.0.3",
    "@types/cookies": "^0.9.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.0.3",
    "nextjs-routes": "^2.2.4",
    "picsum-photos": "^3.0.10",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.15",
    "trpc-panel": "^1.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.20.2"
  },
  "packageManager": "pnpm@9.1.1"
}
